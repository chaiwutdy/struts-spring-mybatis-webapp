<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="tNdcreceivedMapper" class="org.mybatis.spring.mapper.MapperFactoryBean">
   		<property name="sqlSessionFactory" ref="sqlSessionFactory"/>
   		<property name="mapperInterface" value="com.dao.TNdcreceivedMapper" />
	</bean>
	
	<bean id="tNdcreceivedMapperExt" class="org.mybatis.spring.mapper.MapperFactoryBean">
   		<property name="sqlSessionFactory" ref="sqlSessionFactory"/>
   		<property name="mapperInterface" value="com.dao.TNdcreceivedMapperExt" />
	</bean>

</beans>